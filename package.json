{
  "name": "ge-ng-dpc",
  "version": "1.0.0",
  "description": "GE Angular Init",
  "main": "index.js",
  "scripts": {
    "copy-libs": "mkdir www/lib && cp node_modules/angular/angular.min.js www/lib/",
    "gulp-copy-libs": "node_modules/.bin/gulp copy-libs",
    "serve": "node_modules/.bin/http-server -p 8080 www",
    "apiserver": "node node_modules/dpc-angular-server/server.js ./www",
    "test": "karma start",
    "postinstall": "npm run gulp-copy-libs"
  },
  "author": "Flaki",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-route": "^1.5.8"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "chai": "^3.5.0",
    "dpc-angular-server": "flaki/dpc-angular-server",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.2",
    "gulp-concat": "^2.6.1",
    "http-server": "^0.9.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.1.2",
    "phantomjs-prebuilt": "^2.1.13",
    "whatwg-fetch": "^1.0.0"
  }
}
